@charset "utf-8";

.gsc-control-cse { font-family: Arial, sans-serif; background-color: rgb(255, 255, 255); border: 1px solid rgb(255, 255, 255); }

.gsc-control-cse .gsc-table-result { font-family: Arial, sans-serif; width: auto; }

.gsc-resultsHeader { }

.gsc-input { font-size: 16px; }

.gsc-input-box { border: 1px solid rgb(187, 187, 187); background: rgb(255, 255, 255); }

.gsc-search-box .gsc-input > input:focus, .gsc-input-box-focus { border: 1px solid rgb(187, 187, 187); box-shadow: rgba(0, 0, 0, 0.3) 0px 1px 2px inset; outline: none; }

.gsc-search-button-v2 { font-size: 0px; padding: 6px 27px; width: auto; vertical-align: middle; border: 1px solid rgb(0, 0, 0); border-radius: 2px; margin-top: 2px; background-color: rgb(51, 51, 51); }

.gsc-search-button-v2:hover { border-color: rgb(0, 0, 0); background-color: rgb(51, 51, 51); }

.gsc-search-button-v2 svg { fill: rgb(255, 255, 255); }

.gsc-tabHeader.gsc-tabhInactive { color: rgb(68, 68, 68); }

.gsc-tabHeader.gsc-tabhActive { border-bottom: 2px solid rgb(51, 51, 51); color: rgb(51, 51, 51); }

.gsc-refinementHeader { text-decoration: none; color: rgb(68, 68, 68); font-weight: bold; line-height: 27px; margin-right: 0px; }

.gsc-refinementHeader.gsc-refinementhActive { border-bottom: 2px solid rgb(51, 51, 51); color: rgb(51, 51, 51); }

.gs-result .gs-title *, .gsc-cursor-page, .gs-spelling a { color: rgb(68, 68, 68); }

.gs-webResult.gs-result .gs-title:active, .gs-webResult.gs-result .gs-title:active b, .gs-imageResult .gs-title:active, .gs-imageResult .gs-title:active b { color: rgb(119, 119, 119); }

.gs-webResult .gs-snippet, .gs-fileFormatType { color: rgb(51, 51, 51); }

.gsc-webResult.gsc-result.gsc-promotion { background-color: rgb(230, 230, 230); border-color: rgb(204, 204, 204); }

.gs-promotion .gs-title:link, .gs-promotion .gs-title:link *, .gs-promotion .gs-snippet a:link { color: rgb(0, 0, 204); }

.gs-promotion .gs-title:visited, .gs-promotion .gs-title:visited *, .gs-promotion .gs-snippet a:visited { color: rgb(0, 0, 204); }

.gs-promotion .gs-title:hover, .gs-promotion .gs-title:hover *, .gs-promotion .gs-snippet a:hover { color: rgb(68, 68, 68); }

.gs-promotion .gs-title:active, .gs-promotion .gs-title:active *, .gs-promotion .gs-snippet a:active { color: rgb(0, 204, 0); }

.gs-promotion .gs-snippet, .gs-promotion .gs-title .gs-promotion-title-right, .gs-promotion .gs-title .gs-promotion-title-right * { color: rgb(51, 51, 51); }

.gs-promotion .gs-visibleUrl, .gs-webResult .gs-visibleUrl { color: rgb(0, 0, 0); }

.gsc-completion-selected { background: rgb(238, 238, 238); }

.gsc-completion-container { font-family: Arial, sans-serif; font-size: 16px; background: white; border: 1px solid rgb(187, 187, 187); }

.gsc-completion-title { color: rgb(0, 0, 204); }

.gsc-completion-snippet { color: rgb(51, 51, 51); }

.gsc-rating-bar { display: inline-block; width: 54px; height: 10px; background: url("/cse/static/css/cse_snippets_stars.png") -52px -52px no-repeat transparent; margin: 3px 0px 0px; }

.gsc-rating-bar span { display: inline-block; height: 10px; background: url("/cse/static/css/cse_snippets_stars.png") 0px -52px no-repeat transparent; }

.gsc-option-menu-item-highlighted { background-color: rgb(238, 238, 238); }

.gsc-results { background-color: rgb(255, 255, 255); width: auto; }

.gsc-results .gsc-cursor-box { margin: 10px; }

.gsc-results .gsc-cursor-box .gsc-cursor-page { text-decoration: none; color: rgb(51, 51, 51); }

.gsc-results .gsc-cursor-box .gsc-cursor-page:hover { text-decoration: underline; }

.gsc-results .gsc-cursor-box .gsc-cursor-current-page { color: rgb(34, 34, 34); }

.gsc-context-box .gsc-facet-label { width: 65px; padding-left: 2px; text-decoration: underline; color: rgb(51, 51, 51); cursor: pointer; }

.gsc-context-box .gsc-chart { width: 32em; padding: 3px; border-left: 1px solid rgb(119, 119, 119); border-right: 1px solid rgb(119, 119, 119); }

.gsc-context-box .gsc-top { border-top: 1px solid rgb(119, 119, 119); }

.gsc-context-box .gsc-bottom { border-bottom: 1px solid rgb(119, 119, 119); }

.gsc-context-box .gsc-chart div { background: rgb(119, 119, 119); height: 9px; }

.gsc-context-box .gsc-facet-result { color: rgb(51, 51, 51); width: 30px; text-align: right; padding-right: 5px; }

.gs-webResult .gs-title .gs-title.gsc-usr-group-heading { color: rgb(51, 51, 51); cursor: pointer; }

.gs-webResult .gs-title .gs-title.gsc-usr-group-heading b { color: rgb(51, 51, 51); }

.gcsc-find-more-on-google { color: rgb(68, 68, 68); }

.gcsc-find-more-on-google-magnifier { fill: rgb(68, 68, 68); }
